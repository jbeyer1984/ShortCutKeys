{
    "name": "Short Cut Keys",
    "version": "1.0.0",
    "description": "based on current url / with short cut like input",
    "permissions": [
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "http://*/",
        "https://*/"
    ],
    "icons": {
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}